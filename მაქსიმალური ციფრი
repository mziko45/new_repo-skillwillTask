#include <iostream>

using namespace std;

int main()
{
    cout << "მაქსიმალური ციფრის პოვნა";
    
    int   numberOne, numberTwo, numberThree;
    cout << "მიუთითე  პირველი ციფრი"   <<endl;
    cin >> numberOne;
    cout << "მიუთითე მეორე რიცხვი"  <<endl;
    cin >> numberTwo;
    cout << "მიუთითე მესამე ციფრი"  <<endl;
    cin >> numberThree;
    
    if (numberOne > numberTwo && numberOne > numberThree ) {
        cout << " numberOne არის მაქსიმალური ციფრიი "   <<endl;
    } else if ( numberTwo > numberOne && numberTwo > numberThree ) {
        cout << " numberTwo არის მაქსიმალური ციფრი " <<endl;
    }else if (numberThree > numberOne && numberThree > numberTwo) {
        cout << " numberThree არის მაქსიმალური ციფრი "   <<endl;
    } else if ( numberOne == numberTwo && numberTwo == numberThree) {
        cout << " numberOne, numberTwo, numberThree ციფრები ტოლია "  <<endl;
    }else {
        cout << " numberThree არის მაქსიმალური ციფრი "  <<endl;
    }
    

    return 0;
}
